{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/(favicon\\.ico|robots\\.txt|sitemap\\.xml|debug\\.html|_redirects|.*\\.(png|jpg|jpeg|gif|webp|svg|js|css|woff|woff2|ttf|eot|glb))",
      "dest": "/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}